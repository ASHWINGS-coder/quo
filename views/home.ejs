<h1>
    Home / <%= title %>
</h1>

<section id="feed-posts">
    <h1>Posts</h1>
    <% if(locals.user){%>
    <form action="/posts/create" id="new-post-form" method="POST">
        <textarea name="content" id="" cols="30" rows="3" placeholder="TypeHere"></textarea>
        <input type="submit" value="post">
    </form>
    <% } %>
    <div id="posts-list-container">
        <ul>
             <% for(post of posts) {%>
                <%- include('_post') -%>
            <%}%>
        </ul>
       
    </div>
</section>
 


